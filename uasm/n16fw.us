// EN_MDR may be redundant
//
// alu op
//
//   iiiiii ttt sss oooo
// b 101000 001 010 0001
// h a0 a1

<0x00>
@Init:
@InstNoop:
    EN_IR, EN_PC_0, EN_MDR, RAM_LD; // init and noop

<0xa0>
@InstALURegisterTransfer:
    RU_SEL_RS, RU_SEL_RT, RU_EN_RT, // Register
    SEL_ALU,                        // ALU
    EN_IR, EN_PC_0, EN_MDR, RAM_LD; // Fetch next instruction on falling edge

<0xb0>
@InstUnused:
    EN_IR, EN_PC_0, EN_MDR, RAM_LD; // Fetch next instruction on falling edge

